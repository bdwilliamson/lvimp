Package: lvimp
Type: Package
Title: Perform Inference on Summaries of Longitudinal Algorithm-Agnostic Variable Importance
Version: 1.0.0
Authors@R:
    c(person(given = "Brian D.",
             family = "Williamson",
             role = c("aut", "cre"),
             email = "brian.d.williamson@kp.org",
             comment = c(ORCID = "0000-0002-7024-548X"))
